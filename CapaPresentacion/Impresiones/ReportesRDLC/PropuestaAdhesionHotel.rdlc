                <TextRuns>
                  <TextRun>
                    <Value>ATCCO     S.R.L.,     en    adelante     denominada     “La Empresa”    brindará    al
HOTEL </Value>
                    <Style />
                  </TextRun>
                  <TextRun>
                    <Value>=Parameters!nombreHotel.Value</Value>
                    <Style />
                  </TextRun>
                  <TextRun>
                    <Value>, sito en </Value>
